{
  "name": "sampletypescriptserver",
  "version": "1.0.0",
  "description": "typescript server",
  "main": "dist/src/server.js",
  "scripts": {
    "test": "mocha dist/test/**/*.spec.js --reporter spec",
    "start": "npm run serve",
    "build": "npm run build-ts",
    "watch-node": "nodemon dist/src/bootstrap.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "serve": "node bootstrap.js"
  },
  "keywords": [
    "typescript",
    "server"
  ],
  "author": "Danilo Avilez",
  "license": "ISC",
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.3",
    "@types/cors": "^2.8.4",
    "@types/express": "^4.11.1",
    "@types/inversify": "^2.0.33",
    "@types/inversify-express-utils": "^2.0.0",
    "@types/inversify-logger-middleware": "^2.0.0",
    "@types/mocha": "^5.2.0",
    "@types/mongodb": "^3.0.18",
    "@types/reflect-metadata": "^0.1.0",
    "@types/supertest": "^2.0.4",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "supertest": "^3.1.0"
  },
  "dependencies": {
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "express": "^4.16.3",
    "inversify": "^4.13.0",
    "inversify-express-utils": "^6.0.0",
    "inversify-logger-middleware": "^3.1.0",
    "mongodb": "^3.1.0-beta4",
    "reflect-metadata": "^0.1.12"
  }
}
